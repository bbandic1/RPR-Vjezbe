<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import java.net.URL?>

<HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ba.unsa.etf.rpr.lab10.GlavnaController">
   <children>
      <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
         <children>
            <TableView fx:id="gradoviTableView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
               <columns>
                  <TableColumn fx:id="idCol" editable="false" maxWidth="500.0" minWidth="-Infinity" prefWidth="100.0" text="ID" />
                  <TableColumn fx:id="nazivCol" editable="false" maxWidth="500.0" minWidth="-1.0" prefWidth="110.0" text="Naziv" />
                  <TableColumn fx:id="stanovnikaCol" editable="false" maxWidth="500.0" minWidth="-Infinity" prefWidth="120.0" text="Stanovnika" />
                  <TableColumn fx:id="drzavaCol" editable="false" maxWidth="500.0" minWidth="-Infinity" prefWidth="130.0" text="Država" />
               </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
         <HBox.margin>
            <Insets bottom="5.0" left="5.0" />
         </HBox.margin>
      </HBox>
      <VBox maxHeight="1.7976931348623157E308" maxWidth="200.0" minHeight="-Infinity" minWidth="-Infinity" HBox.hgrow="ALWAYS">
         <children>
            <ToolBar maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" orientation="VERTICAL" VBox.vgrow="NEVER">
               <items>
                  <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#dodajGradOnClick" text="Dodaj grad">
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </Button>
                  <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#dodajDrzavuOnClick" text="Dodaj državu">
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </Button>
                  <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#izmijeniGradOnClick" text="Izmijeni grad">
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </Button>
                  <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#obrisiGradOnClick" text="Obriši grad">
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </Button>
               </items>
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" />
               </VBox.margin>
            </ToolBar>
         </children>
      </VBox>
   </children>
</HBox>